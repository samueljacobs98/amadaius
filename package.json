{
  "name": "amadaius",
  "version": "1.3.8",
  "description": "Amadaius is a simple and lightweight prompt-templating engine.",
  "author": "Samuel Jacobs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "license": "MIT",
  "packageManager": "pnpm@10.23.0",
  "save-exact": true,
  "homepage": "https://github.com/samueljacobs98/amadaius#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samueljacobs98/amadaius.git"
  },
  "bugs": {
    "url": "https://github.com/samueljacobs98/amadaius/issues"
  },
  "keywords": [
    "prompt",
    "template",
    "engine",
    "llm",
    "prompt engineering"
  ],
  "scripts": {
    "lint": "eslint './src/**/*.{js,ts}'",
    "build": "tsc -p tsconfig.json",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:types": "tsd -f test/test-d",
    "format:check": "prettier --check './src/**/*.{ts,js,json,md}'",
    "format:fix": "prettier --write './src/**/*.{ts,js,json,md}'",
    "prepare": "husky"
  },
  "dependencies": {
    "handlebars": "4.7.8",
    "lodash": "4.17.21"
  },
  "peerDependencies": {
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/handlebars": "4.0.40",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.13",
    "@types/node": "22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jest": "29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "29.2.5",
    "tsd": "^0.31.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1"
  }
}
