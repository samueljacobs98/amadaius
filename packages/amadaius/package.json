{
  "name": "amadaius",
  "description": "Amadaius is a simple and lightweight prompt-templating engine.",
  "version": "1.3.9",
  "author": "Samuel Jacobs",
  "bugs": "https://github.com/samueljacobs98/amadaius/issues",
  "dependencies": {
    "handlebars": "4.7.8",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@config/eslint": "workspace:*",
    "@config/typescript": "workspace:*",
    "@types/handlebars": "4.0.40",
    "@types/jest": "catalog:",
    "@types/lodash": "4.17.13",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "jest": "catalog:",
    "ts-jest": "29.4.5",
    "tsd": "^0.31.2",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://github.com/samueljacobs98/amadaius#readme",
  "keywords": [
    "engine",
    "llm",
    "prompt",
    "prompt engineering",
    "template"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "packageManager": "pnpm@10.23.0",
  "peerDependencies": {
    "zod": "3.23.8"
  },
  "repository": "samueljacobs98/amadaius.git",
  "save-exact": true,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "format:check": "prettier --check './src/**/*.{ts,js,json,md}'",
    "format:fix": "prettier --write './src/**/*.{ts,js,json,md}'",
    "lint": "eslint './src/**/*.{js,ts}'",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:types": "pnpm build && tsd -f test/test-d",
    "typecheck": "tsc --noEmit"
  },
  "types": "dist/index.d.ts"
}
