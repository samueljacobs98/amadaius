{
  "name": "amadaius",
  "version": "1.3.8",
  "description": "Amadaius is a simple and lightweight prompt-templating engine.",
  "author": "Samuel Jacobs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "license": "MIT",
  "packageManager": "pnpm@10.23.0",
  "save-exact": true,
  "homepage": "https://github.com/samueljacobs98/amadaius#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samueljacobs98/amadaius.git"
  },
  "bugs": {
    "url": "https://github.com/samueljacobs98/amadaius/issues"
  },
  "keywords": [
    "prompt",
    "template",
    "engine",
    "llm",
    "prompt engineering"
  ],
  "scripts": {
    "lint": "eslint './src/**/*.{js,ts}'",
    "build": "tsc -p tsconfig.json",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:types": "tsd -f test/test-d",
    "typecheck": "tsc --noEmit",
    "format:check": "prettier --check './src/**/*.{ts,js,json,md}'",
    "format:fix": "prettier --write './src/**/*.{ts,js,json,md}'"
  },
  "dependencies": {
    "handlebars": "4.7.8",
    "lodash": "4.17.21"
  },
  "peerDependencies": {
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@config/eslint": "workspace:",
    "@config/typescript": "workspace:",
    "@types/handlebars": "4.0.40",
    "@types/jest": "catalog:",
    "@types/lodash": "4.17.13",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "jest": "catalog:",
    "ts-jest": "29.4.5",
    "tsd": "^0.31.2",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:"
  }
}
